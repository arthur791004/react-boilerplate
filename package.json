{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for quickly creating a new project of react",
  "main": "index.js",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "prod": "yarn run start:prod",
    "prebuild": "yarn run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config ./internals/webpack/prod.config.js",
    "build:clean": "rimraf ./build",
    "test": "NODE_ENV=test jest --config ./internals/jest/config.js --passWithNoTests",
    "test:coverage": "yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "lint": "yarn run lint-eslint .",
    "lint-eslint": "eslint --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006"
  },
  "repository": "https://github.com/arthur791004/react-boilerplate",
  "author": "arthur <arthur791004@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@hot-loader/react-dom": "~16.8.6",
    "express": "~4.17.1",
    "prop-types": "~15.7.2",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-hot-loader": "~4.12.10",
    "react-router-dom": "~5.0.1",
    "styled-components": "~4.3.2",
    "webpack-hot-middleware": "~2.25.0"
  },
  "devDependencies": {
    "@babel/core": "~7.5.4",
    "@babel/plugin-proposal-class-properties": "~7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "~7.5.4",
    "@babel/preset-env": "~7.5.4",
    "@babel/preset-react": "~7.0.0",
    "@storybook/addon-knobs": "~5.1.9",
    "@storybook/addon-viewport": "~5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@svgr/webpack": "~4.3.1",
    "babel-eslint": "~10.0.2",
    "babel-jest": "~24.8.0",
    "babel-loader": "~8.0.6",
    "babel-plugin-module-resolver": "~3.2.0",
    "babel-plugin-styled-components": "~1.10.6",
    "cross-env": "~5.2.0",
    "eslint": "~6.0.1",
    "eslint-config-airbnb": "~17.1.1",
    "eslint-config-prettier": "~6.0.0",
    "eslint-import-resolver-babel-module": "~5.1.0",
    "eslint-plugin-import": "~2.18.0",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.14.2",
    "eslint-plugin-react-hooks": "~1.6.1",
    "html-loader": "~0.5.5",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~3.0.2",
    "jest": "~24.8.0",
    "lint-staged": "~9.2.0",
    "prettier": "~1.18.2",
    "rimraf": "~2.6.3",
    "uglifyjs-webpack-plugin": "~2.1.3",
    "url-loader": "~2.0.1",
    "webpack": "~4.35.3",
    "webpack-cli": "~3.3.5",
    "webpack-dev-middleware": "~3.7.0"
  }
}
